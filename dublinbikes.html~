<script>
   function initializeMap() {
     
  var mapProp = {
    center:new google.maps.LatLng(53.2734,-7.778320310000026),
    zoom:8,
    mapTypeId:google.maps.MapTypeId.ROADMAP
  };
 //create map object
  var map=new google.maps.Map(document.getElementById("stationmap"), mapProp);
       
//  var contentString = ;    
       
//  var infowindow = new google.maps.InfoWindow({
//    content: contentString
//  });
}
google.maps.event.addDomListener(window, 'load', initializeMap);
    

